{
  "name": "lol-ranks-bot",
  "version": "0.1.4",
  "description": "A Discord bot to assign roles based on League of Legends Rank",
  "keywords": [
    "Discord",
    "Bot",
    "League of Legends",
    "LoL"
  ],
  "license": "CC BY-NC-SA 4.0",
  "author": "AlthalusAvan feat. simwai",
  "main": "bot.js",
  "scripts": {
    "always-debug": "nodemon -e 'js' --inspect --trace-warnings ./src/bot.js",
    "debug": "node --inspect --trace-warnings ./src/bot.js",
    "lint": "eslint --ext .js,.json,.eslintrc,.babelrc --fix .",
    "start": "node bot.js",
    "stop-debug": "taskkill /F /IM node.exe"
  },
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "bottleneck": "^2.19.5",
    "cron": "^2.0.0",
    "discord-api-types": "^0.32.1",
    "discord.js": "^13.0.0",
    "i18n": "^0.14.2",
    "lowdb": "^1.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2"
  }
}
